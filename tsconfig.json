{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"declaration": true,
		"declarationDir": "dist/types",
		"emitDeclarationOnly": false,
		"outDir": "dist",
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"paths": {
			"chase-ui": ["./src"]
		}
	},
	"include": ["src"]
}
